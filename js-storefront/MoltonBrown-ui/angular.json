{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "moltonbrown": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/moltonbrown",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles"
              ]
            },
            "assets": [
              "src/favicon.ico",
              "./src/assets/fonts/Molengo.ttf",
              "./src/assets/fonts/trebuc.ttf",
              "src/assets",
              "src/manifest.json"
            ],
            "styles": [
              "src/styles/all.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles.scss",
              "node_modules/slick-carousel/slick/slick.scss",
              "node_modules/slick-carousel/slick/slick-theme.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "./src/assets/js/amp-min.js",
              "./src/assets/js/yre05t09.js",
              "./src/assets/js/handlebar.min.js",
              "./src/assets/js/handlebars_helpers.js",
              "./src/assets/js/utils.js",
              "./src/assets/js/acc-template-text.min.js",
              "./src/assets/js/banner/acc-template-banner.min.js",
              "./src/assets/js/blog/acc-template-blog.min.js",
              "./src/assets/js/blog/acc-template-snippet.min.js",
              "./src/assets/js/card/acc-template-card.min.js",
              "./src/assets/js/cardList/acc-template-cardList.min.js",
              "./src/assets/js/externalBlock/acc-template-externalBlock.min.js",
              "./src/assets/js/homepage/acc-template-homepage.min.js",
              "./src/assets/js/image/acc-template-image.min.js",
              "./src/assets/js/promoBanner/acc-template-promoBanner.min.js",
              "./src/assets/js/promoBannerSection/acc-template-promoBannerSection.min.js",
              "./src/assets/js/slider/acc-template-slider.min.js",
              "./src/assets/js/splitBlock/acc-template-splitBlock.min.js",
              "./src/assets/js/text/acc-template-text.min.js",
              "./src/assets/js/video/acc-template-video.min.js",
              "./src/assets/js/bvApi.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/slick-carousel/slick/slick.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "moltonbrown:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "moltonbrown:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "moltonbrown:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.json"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "moltonbrown-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "moltonbrown:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "moltonbrown:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "moltonbrown"
}